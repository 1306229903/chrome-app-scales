{
  "name" : "Serial Port App" ,
  "description" : "让普通网页也能读取使用串行接口接入到电脑的设备的数据，例如电子秤的读数。" ,
  "version" : "1.0.1" ,
  "manifest_version" : 2 ,
  "permissions" : [
    "serial"
  ] ,
  "app" : {
    "background" : {
      "scripts" : [
        "./bundle/commons.js" ,
        "./bundle/background.js"
      ]
    }
  } ,
  "externally_connectable" : {
    "matches" : [
      "*://localhost:*/*" ,
      "*://127.0.0.1:*/*" ,
      "*://192.168.10.18:*/*" ,
      "*://*.yi-express.com:*/*" ,
      "*://*.yi-ex.com:*/*"
    ]
  }
}
